{
  "api": {
    "baseURL": "http://127.0.0.1",
    "namespace": "domain",
    "paging": {
      "defaultLimit": 20,
      "defaultOffset": 0
    },
    "port": 9002,
    "sorting": {
      "defaultOrder": "desc",
      "defaultProp": "created_on"
    },
    "version": 1
  },
  "logger": {
    "app": {
      "enabled": "true",
      "level": "info"
    },
    "http": {
      "enabled": {
        "request": "true",
        "response": "true"
      },
      "label": "http",
      "level": "info"
    },
    "repo": {
      "enabled": "false",
      "label": "repository",
      "level": "info"
    },
    "validation": {
      "enabled": "false",
      "label": "validation",
      "level": "info"
    }
  },
  "service": {
    "baseURL": "",
    "headers": {},
    "timeout": 1000
  }
}
