{
  "api": {
    "baseURL": "API_BASE_URL",
    "port": "API_PORT"
  },
  "logger": {
    "app": {
      "enabled": "LOGGER_APP_ENABLED",
      "level": "LOGGER_APP_LEVEL"
    },
    "http": {
      "enabled": {
        "request": "LOGGER_HTTP_REQUEST_ENABLED",
        "response": "LOGGER_HTTP_RESPONSE_ENABLED"
      },
      "level": "LOGGER_HTTP_LEVEL"
    },
    "repo": {
      "enabled": "LOGGER_REPO_ENABLED",
      "level": "LOGGER_REPO_LEVEL"
    },
    "services": {
      "user": {
        "enabled": "LOGGER_USER_SERVICE_ENABLED",
        "level": "LOGGER_USER_SERVICE_LEVEL"
      }
    },
    "validation": {
      "enabled": "LOGGER_VALIDATION_ENABLED",
      "level": "LOGGER_VALIDATION_LEVEL"
    }
  },
  "services": {
    "user": {
      "api": {
        "baseURL": "USER_SERVICE_BASE_URL",
        "headers": "USER_SERVICE_HEADERS",
        "timeout": "USER_SERVICE_TIMEOUT"
      }
    }
  }
}
