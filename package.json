  },
  "homepage": "https://github.com/jasonsites/nsk-koa#readme",
  "dependencies": {
    "bluebird": "^3.5.0",
    "boom": "^4.2.0",
    "graphql": "^0.9.2",
    "jsonwebtoken": "^7.3.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-bunyan-logger": "^1.3.0",
