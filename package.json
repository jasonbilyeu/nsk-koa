{
  "name": "nsk-koa",
  "version": "0.2.0",
  "description": "Node Starter Kit for Koa API Applications",
  "main": "src/index.js",
  "scripts": {
    "preinstall": "npm prune",
    "coverage": "nyc npm t",
    "dev": "NODE_CONFIG_DIR=./test/config nodemon src/index.js",
    "lint": "eslint src test",
    "release": "standard-version",
    "sec": "nsp check --output summary",
    "test": "mocha --recursive --exit test/tests/**/*.test.js"
  },
  "keywords": [
    "api",
    "http",
    "koa",
    "node",
    "starter kit",
    "web"
  ],
  "author": "Jason Sites <email@jasonsites.com> (https://github.com/jasonsites)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonsites/nsk-koa.git"
  },
  "bugs": {
    "url": "https://github.com/jasonsites/nsk-koa/issues"
  },
  "homepage": "https://github.com/jasonsites/nsk-koa#readme",
  "nyc": {
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "temp-directory": "./coverage/.nyc_output"
  },
  "dependencies": {
    "app-container": "^1.0.0",
    "bluebird": "^3.5.1",
    "boom": "^7.1.1",
    "bunyan": "^1.8.12",
    "config": "^1.29.4",
    "joi": "^13.1.2",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.5",
    "transformalizer": "^1.1.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "faker": "^4.1.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.12",
    "nsp": "^3.1.0",
    "nyc": "^11.4.1",
    "sinon": "^4.3.0",
    "standard-version": "^4.3.0",
    "supertest": "^3.0.0"
  }
}
