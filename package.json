{
  "name": "nsk-koa",
  "version": "0.0.3",
  "description": "Node Starter Kit (Koa)",
  "main": "src/index.js",
  "scripts": {
    "preinstall": "npm prune",
    "build": "rm -rf dist/* && babel src -d dist --copy-files",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha --recursive test/**/*.test.js test/**/**/*.test.js",
    "debug": "node --inspect --debug-brk dist/index.js",
    "postversion": "npm run build",
    "release": "npm run build && standard-version",
    "serve": "node dist/index.js",
    "start": "nodemon src/index.js --exec babel-node",
    "sec": "nsp check --output summary",
    "test": "mocha --compilers js:babel-register test/tests/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonsites/nsk-koa.git"
  },
  "keywords": [],
  "author": "Jason Sites <email@jasonsites.com> (https://github.com/jasonsites)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonsites/nsk-koa/issues"
  },
  "homepage": "https://github.com/jasonsites/nsk-koa#readme",
  "dependencies": {
    "ajv": "^5.0.1",
    "app-container": "^0.4.3",
    "baobab": "^2.4.3",
    "bluebird": "^3.5.0",
    "boom": "^4.2.0",
    "bunyan": "^1.8.10",
    "graphql": "^0.9.2",
    "joi": "^10.5.0",
    "jsonwebtoken": "^7.3.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-graphql": "^0.6.2",
    "koa-router": "^7.1.1",
    "lodash": "^4.17.4",
    "source-map-support": "^0.4.14"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.2.2",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.0.1",
    "faker": "^4.1.0",
    "mocha": "^3.4.1",
    "nock": "^9.0.13",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "sinon": "^2.0.0",
    "standard-version": "^4.0.0",
    "superagent": "^3.5.2",
    "supertest": "^3.0.0"
  }
}
