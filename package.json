{
  "name": "nsk-koa",
  "version": "0.0.3",
  "description": "Node Starter Kit (Koa)",
  "main": "src/index.js",
  "scripts": {
    "preinstall": "npm prune",
    "build": "rm -rf dist/* && cp -a src/* dist/",
    "coverage": "istanbul cover node_modules/.bin/_mocha --recursive test/**/*.test.js",
    "debug": "node --inspect --debug-brk dist/index.js",
    "release": "npm run build && standard-version",
    "serve": "node dist/index.js",
    "start": "node src/index.js",
    "sec": "nsp check --output summary",
    "test": "mocha --recursive test/tests/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonsites/nsk-koa.git"
  },
  "keywords": [],
  "author": "Jason Sites <email@jasonsites.com> (https://github.com/jasonsites)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonsites/nsk-koa/issues"
  },
  "homepage": "https://github.com/jasonsites/nsk-koa#readme",
  "dependencies": {
    "ajv": "^5.2.2",
    "app-container": "^0.4.7",
    "baobab": "^2.4.3",
    "bluebird": "^3.5.0",
    "boom": "^5.2.0",
    "bunyan": "^1.8.10",
    "graphql": "^0.10.5",
    "joi": "^10.6.0",
    "jsonwebtoken": "^7.4.1",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-router": "^7.2.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chalk": "^2.0.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "faker": "^4.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "nock": "^9.0.14",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.7.0",
    "sinon": "^2.4.1",
    "standard-version": "^4.2.0",
    "superagent": "^3.5.2",
    "supertest": "^3.0.0"
  }
}
