{
  "name": "nsk-koa",
  "version": "0.0.1",
  "description": "Node Starter Kit (Koa)",
  "main": "src/index.js",
  "scripts": {
    "preinstall": "npm prune",
    "build": "babel src -d dist",
    "prestart": "",
