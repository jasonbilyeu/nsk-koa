{
  "name": "nsk-koa",
  "version": "0.3.3",
  "description": "Node Starter Kit for Koa API Applications",
  "main": "src/index.js",
  "scripts": {
    "preinstall": "npm prune",
    "coverage": "nyc npm t",
    "dev": "NODE_CONFIG_DIR=./test/config nodemon src/index.js",
    "lint": "eslint src test",
    "test": "mocha --recursive --exit 'test/tests/**/*.test.js'"
  },
  "keywords": [
    "api",
    "http",
    "koa",
    "node",
    "starter kit",
    "web"
  ],
  "author": "Jason Sites <email@jasonsites.com> (https://github.com/jasonsites)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonsites/nsk-koa.git"
  },
  "bugs": {
    "url": "https://github.com/jasonsites/nsk-koa/issues"
  },
  "homepage": "https://github.com/jasonsites/nsk-koa#readme",
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "temp-directory": "./coverage/.nyc_output"
  },
  "dependencies": {
    "@hapi/joi": "^15.0.1",
    "app-container": "^1.1.2",
    "bluebird": "^3.5.4",
    "boom": "^7.3.0",
    "bunyan": "^1.8.12",
    "config": "^3.1.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.14",
    "transformalizer": "^1.1.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chance": "^1.0.18",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.0",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2"
  }
}
